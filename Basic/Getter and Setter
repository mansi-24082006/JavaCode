class Worker{
    private int id;
    private String name;
    public String getName(){
        return name;
    }
    public void setName(String n){
         name = n;
    }
    public void setId(int i){
       id = i;
    }
    public int getId(){
        return id;
    }
}
public class Main {
    public static void main(String[] args) {
        Worker girl = new Worker();
        girl.setName("Dimple");
        System.out.println(girl.getName());
        girl.setId(133);
        System.out.println(girl.getId());
    }
}


*******Output
Dimple
133
