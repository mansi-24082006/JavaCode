public class Main {
    static int fibonacci(int n) {
       /*if(n==1){
           return 0;
       }
       else if(n==2){
           return 1;
       }*/
       if(n==1 || n==2){
           return n-1;
       }
       else {
           return fibonacci(n-1)+fibonacci(n-2);
       }
        
    }
    
    public static void main(String[] args) {
        int res =  fibonacci(8);
        System.out.println(res);
    }
}


*******Output
13
